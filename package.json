{
  "name": "curso-cypress-v2",
  "version": "1.0.0",
  "description": "Repositório da versão 2 do curso básico de Cypress da Escola Talking About Testing",
  "main": "src/index.html",
  "scripts": {
    "cy:open": "npx cypress open --config-file=cypress.config.js",
    "cy:open:mobile": "npx cypress open --config-file=cypress.config.js --config viewportWidth=410 viewportHeight=860",
    "test": "npx cypress run --config-file=cypress.config.js",
    "test:mobile": "npx cypress run --config-file=cypress.config.js --config viewportWidth=410 viewportHeight=860"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlsf82/cypress-basico-v2.git"
  },
  "keywords": [
    "Testing",
    "Cypress.io",
    "Talking About Testing",
    "Cypress Básico",
    "Escola TAT"
  ],
  "devDependencies": {
    "cypress": "^13.13.3"
  }
}
